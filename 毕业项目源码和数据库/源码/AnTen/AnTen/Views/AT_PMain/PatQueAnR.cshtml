        if(!$(this).find('a').size()){
            return false;
        }
        var _index = $(this).index();
        $(this).addClass('now').siblings().removeClass('now');
        $(".apppage .tabs_box .tbright .inner").eq(_index).show().siblings().hide();
        $('#qa_search_text').val('');
    })
    $('.apppage .tbleft .item:first').click();
    $(".apppage .tabs_box .tbright .scroll .list1 .title2").bind('click',function(){
        if($(this).parent().hasClass('now')){
           $(this).parent().removeClass("now");
        }else{
            $(this).parent().addClass("now");
        }

    });
    $('#qa_search').click(function(){
        var keywords = $.trim($('#qa_search_text').val());
        var _index = $('.tbleft .item.now').index();
        if(!keywords){
            $('.tbright .inner').eq(_index).show().siblings().hide();
            return false;
        }
        keywords = keywords.split(' ');
        $('.qa_search .list1').html('');
        $('.tbright .scroll .list1 .item').each(function(index){
            var _clone = '';
            for (var i = 0; i < keywords.length; i++) {
                if( ! keywords[i]){
                    continue;
                }
                if($(this).text().indexOf(keywords[i]) >= 0){
                    $('.qa_search .list1').append($(this).clone(true));
                    break;
                }
            }
        })
        for (var i = 0; i < keywords.length; i++) {
            if( ! keywords[i]){
                continue;
            }
            var reg = new RegExp(keywords[i],'g');
            var _html = $('.qa_search .list1').html().replace(reg, '<i>'+keywords[i]+'</i>');
            $('.qa_search .list1').html(_html);
        }

        $(".apppage .tabs_box .tbright .inner.qa_search").show().siblings().hide();
        if(!$(".apppage .tabs_box .tbright .inner.qa_search .list1 .item").size()){
            $('.qa_search .list1').append('<div class="item" style="text-align:center;">暂无相关数据</div>');
        }
        $(".apppage .tabs_box .tbright .qa_search .list1 .title2").bind('click',function(){
            if($(this).parent().hasClass('now')){
               $(this).parent().removeClass("now");
            }else{
                $(this).parent().addClass("now");
            }
        });
    })
    $('#qa_search_text').on('keyup', function(e){
        if(e.keyCode==13){
            $('#qa_search').click();
        }
    });
})

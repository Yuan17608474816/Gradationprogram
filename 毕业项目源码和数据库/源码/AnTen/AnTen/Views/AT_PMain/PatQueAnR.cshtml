                    <script>
$('#techang,#keshiname,#title').on('keyup', function(e){
    if(e.keyCode==13){
        $('#search-btn').click();
    }
});
/*出诊信息查询*/
var val,ibox,val1;
$(".apppage .tabs_box .tbleft .part2 .item").hover(function(){
	ibox = $(this).find('input');
	val = ibox.val();
	val1 = ibox.attr("value");
	if(ibox.val() == "" || ibox.val() == val1){
	   ibox.val("");
	}else{
		ibox.focus();
	}

	ibox.select();
});

$(".apppage .tabs_box .tbleft .part2 .item input").bind('blur',function(){
	var val = $(this).val();
	if(val=="" || val==$(this).attr("value")){
		$(this).val($(this).attr("value"));
	}
});
$('#search-btn').click(function(){
    var techang = $('#techang').val();
    var keshiname = $('#keshiname').val();
    var title = $('#title').val();

    techang = (techang != '疾病名称') ? techang : '';
    keshiname = (keshiname != '科室名称') ? keshiname : '';
    title = (title != '医师名称') ? title : '';

    var _url = '/department_ims/visitsearch.html';
    _url = _url + '?techang=' + encodeURI(techang) + '&keshiname=' + encodeURI(keshiname) + '&title=' + encodeURI(title);

    window.location.href = _url;
});
